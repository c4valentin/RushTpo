<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="beastandroiddev.rushtpo">


    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".utils.RushTpoApplication">
        <activity android:name=".activities.SplashScreenActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".activities.MainActivity"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait" />


        <activity android:name=".activities.BrotherPagerActivity"
            android:screenOrientation="portrait"
            android:theme="@style/CustomDialogActivity"
            />

        <activity android:name=".activities.SwitchCampusActivity"
            android:theme="@style/NoActionBar"
            android:screenOrientation="portrait"/>

        <activity android:name=".activities.EventPhotoActivity"
            android:screenOrientation="portrait"
            android:theme="@style/CustomDialogTheme"/>

        <activity android:name=".activities.YoutubePlayerActivity"
            android:theme="@style/YoutubeDialogCustom"/>

        <activity android:name=".activities.CampusMapActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBar"
            />

        <activity android:name=".activities.MapActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoActionBar"/>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBS6zaRUiQYGWXhm_waSQx04nx7uHs5Q9A"/>



    </application>

</manifest>